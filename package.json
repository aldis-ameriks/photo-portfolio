{
  "name": "photography-portfolio",
  "description": "Personal photography portfolio",
  "version": "1.0.0",
  "author": "Aldis Ameriks",
  "scripts": {
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.23",
    "@mdx-js/react": "^1.0.23",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.19.12",
    "gatsby-image": "^2.2.40",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-lodash": "^3.1.20",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.21",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.14",
    "lodash": "^4.17.15",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-images": "^0.5.19",
    "react-spring": "^8.0.27",
    "styled-components": "^5.0.1",
    "styled-theming": "^2.2.0",
    "typeface-fira-sans": "^0.0.75",
    "typeface-montserrat": "^0.0.75"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.1",
    "cypress": "^3.8.3",
    "cypress-testing-library": "^3.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.9.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "gatsby-cypress": "^0.2.22",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.7"
  },
  "homepage": "https://aldisameriks.photos",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/photography-portfolio/issues"
  },
  "keywords": [
    "gatsby",
    "photography"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/photography-portfolio.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ext .js,.jsx --ignore-path .gitignore",
      "git add"
    ]
  }
}
